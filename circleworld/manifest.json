{
  "manifest_version": 2,
  "name": "Circleworld",
  "description": "This extension will crop all images to a circle.",
  "version": "1.0",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "icons": {
    "32": "images/circleworld32.png",
    "64": "images/circleworld64.png",
    "128": "images/circleworld128.png"
  },
  "page_action": {
    "default_title": "Circleworld",
    "default_icon": "images/circleworld64.png"
  },
  "homepage_url": "https://markiewagner.com",
  "web_accessible_resources": [
    "js/jquery-2.1.1.min.js"
  ]
}
